{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Rabbit All",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/app/rabbit/.", // 运行当前文件
      "cwd": "${workspaceFolder}/app/rabbit", // 设置工作目录为项目根目录
      "args": [
        "run",
        "all",
        "--log-level=debug"
      ], // 传递给程序的参数
      "env": {
        "GO111MODULE": "on", // 强制启用模块模式
        "GO_ENV": "development"
      }, // 环境变量
      "showLog": true // 显示调试日志
    },
  ]
}